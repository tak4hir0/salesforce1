    <fields>
        <fullName>DomainReport__c</fullName>
        <formula>HYPERLINK(&quot;/00O10000002pxXP?pv0=&quot; &amp;  Domain__c , &quot;ドメインが一致するアドレス一覧&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>レポート</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Domain__c</fullName>
        <description>Formula: Company domain from email | Question | Customer Community | Salesforce.com
https://success.salesforce.com/questionDetail?qId=a1X30000000JF94EAG</description>
        <formula>SUBSTITUTE(Email__c, LEFT(Email__c, FIND(&quot;@&quot;, Email__c)), NULL)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>メールアドレスの@以降をドメインとして表示します</inlineHelpText>
        <label>ドメイン</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Email__c</fullName>
        <externalId>true</externalId>
        <label>メールアドレス</label>
        <required>true</required>
        <type>Email</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>MailTransmissionCheck__c</fullName>
        <formula>IF(NonEmail__c =  Email__c, &quot;不達&quot;, &quot;送信可能&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>メール送信チェック</label>
        <type>Text</type>
    </fields>
