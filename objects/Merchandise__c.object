<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <deploymentStatus>Deployed</deploymentStatus>
    <fields>
        <fullName>Date__c</fullName>
        <label>日付</label>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Description__c</fullName>
        <label>備考</label>
        <required>true</required>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Field1__c</fullName>
        <label>複数選択リスト</label>
        <picklist>
            <picklistValues>
                <fullName>A</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>B</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>C</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>D</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>E</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>MultiselectPicklist</type>
        <visibleLines>4</visibleLines>
    </fields>
    <fields>
        <fullName>Field2__c</fullName>
        <formula>IF(INCLUDES(Field1__c, &quot;A&quot;), &quot;A&quot;, &quot;&quot;) &amp; IF(INCLUDES(Field1__c, &quot;B&quot;), &quot;B&quot;, &quot;&quot;) &amp; IF(INCLUDES(Field1__c, &quot;C&quot;), &quot;C&quot;, &quot;&quot;) &amp; IF(INCLUDES(Field1__c, &quot;D&quot;), &quot;D&quot;, &quot;&quot;) &amp; IF(INCLUDES(Field1__c, &quot;E&quot;), &quot;E&quot;, &quot;&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>複数選択リストをテキスト</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Field3__c</fullName>
        <label>選択リスト</label>
        <picklist>
            <picklistValues>
                <fullName>A</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>B</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>C</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>D</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>E</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Field4__c</fullName>
        <formula>IF(CONTAINS(Field2__c ,  TEXT(Field3__c)),&quot;ある&quot;, &quot;ない&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>複数選択リストに値があるか？</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Field5__c</fullName>
        <label>取引先</label>
        <referenceTo>Account</referenceTo>
        <relationshipName>Merchandise</relationshipName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Past__c</fullName>
        <formula>IF(Date__c &lt;  TODAY(), &quot;過去&quot;, &quot;未来&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>過去</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Price__c</fullName>
        <label>Price</label>
        <precision>18</precision>
        <required>true</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Total_Inventory__c</fullName>
        <label>Total Inventory</label>
        <precision>18</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
    </fields>
    <label>コンベア</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <label>Name</label>
        <type>Text</type>
    </nameField>
    <pluralLabel>Merchandise</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
</CustomObject>
